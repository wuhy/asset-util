{
  "name": "asset-util",
  "version": "0.1.0",
  "description": "A node utility to provide useful asset utilities, e.g., rewrite resource url.",
  "main": "index.js",
  "scripts": {
    "test": "npm run unit",
    "unit": "mocha test/tasks/*.spec.js",
    "cover": "npm run cover-local && npm run coveralls",
    "cover-local": "istanbul cover _mocha test/tasks/*.spec.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "pre-release": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wuhy/asset-util.git"
  },
  "bugs": {
    "url": "http://github.com/wuhy/asset-util/issues"
  },
  "keywords": [
    "asset",
    "utility",
    "rewrite",
    "replace",
    "url",
    "node"
  ],
  "author": "sparklewhy@gmail.com",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.2",
    "minimatch": "^3.0.3"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.3"
  }
}
